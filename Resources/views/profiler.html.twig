{% if collector.count > 0 %}
    <div id="pusher_profiler">
        {% set firstRequest = false %}
        {% for i, message in collector.logs %}
            {% set key = message.triggerKey %}
            {% set params = message.parameters %}
            <div id="key_{{ i }}" class="key">
                <h3>Method: <em>{{ key }}</em></h3>
                <h4>Parameters</h4>
                <table>
                    <thead>
                    <tr>
                        <th>Key</th>
                        <th>Value</th>
                    </tr>
                    </thead>
                    <tbody>
                    {% for key, value in params %}
                        <tr>
                            <td>{{ key }}</td>
                            <td>{{ value|json_encode }}</td>
                        </tr>
                    {% endfor %}
                    </tbody>
                </table>
            </div>
        {% endfor %}
    </div>
{% else %}
    <p><em>No logs</em></p>
{% endif %}
